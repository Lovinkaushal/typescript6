{
  "name": "typescript-6",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run build && pm2 start build/index.js --name 'typescript-6' -prod",
    "restart": "npm run build && pm2 restart typescript-6",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "prestart": "mkdir -p build/template && cp -a src/template build/"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/moment": "^2.13.0",
    "@types/nodemon": "^1.19.2",
    "@types/swagger-ui-express": "^4.1.3",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.1",
    "multer": "^1.4.5-lts.1",
    "node": "^20.2.0",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "router": "^1.3.8",
    "stripe": "^12.7.0",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.6.3",
    "ts": "^0.2.2",
    "ts-node": "^10.9.1",
    "tsoa": "^5.1.1"
  }
}
